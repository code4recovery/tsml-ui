{
  "compilerOptions": {
    "allowJs": false,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "lib": ["ES2021.String", "DOM"],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "public",
    "strict": true,
    "strictNullChecks": true,
    "target": "ES2015"
  },
  "include": ["index.d.ts", "src/app.tsx", "src/**/*.ts"]
}
